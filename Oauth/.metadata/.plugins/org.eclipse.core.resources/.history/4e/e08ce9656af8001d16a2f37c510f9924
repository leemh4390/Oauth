<script>
	let email = localStorage.getItem('email');

	$(document).ready(function(){
		
	if(email != null){
		$('input[name="email"]').remove();
		let tags = "<span>"+email+"<span>";
		$('.email > tr').append(tags);
	}else{
		$('input[name="email"]').val();
	}
		
		
		
	});
</script>
